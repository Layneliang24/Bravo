{
  "name": "bravo-frontend",
  "version": "1.0.0",
  "description": "Bravo 前端应用",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:skip-check": "vite build",
    "preview": "vite preview",
    "test": "npx vitest",
    "test:ui": "npx vitest --ui",
    "test:coverage": "npx vitest --coverage",
    "test:ci": "npx vitest --coverage --run --passWithNoTests",
    "test:unit:fast": "npx vitest --run --passWithNoTests",
    "test:watch": "npx vitest --watch",
    "test:coverage-verify": "node tests/verify-coverage.js",
    "feature:map": "node ../scripts/buildFeatureMap.js",
    "feature:uncovered": "node -e \"const report = require('./feature-test-map.json'); report.uncovered.forEach(f => console.log(`❌ ${f.id}: ${f.desc}`));\"",
    "feature:validate": "VALIDATE_FEATURE_COVERAGE=true npm run test:coverage",
    "test:feature-mapping": "ENFORCE_FEATURE_MAPPING=true npm test",
    "lint": "eslint . --fix",
    "type-check": "vue-tsc --noEmit",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "element-plus": "^2.4.4",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "v8-to-istanbul": "^9.3.0",
    "vue": "^3.3.8",
    "vue-demi": "^0.14.10",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/vue3": "^8.6.14",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.9.0",
    "@types/nprogress": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "@vue/shared": "^3.5.21",
    "@vue/test-utils": "^2.4.2",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^9.33.0",
    "happy-dom": "^18.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^22.1.0",
    "picocolors": "^1.0.0",
    "prettier": "^3.1.0",
    "storybook": "^8.6.14",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^5.2.2",
    "vite": "^7.1.5",
    "vitest": "^0.34.6",
    "vue-tsc": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
